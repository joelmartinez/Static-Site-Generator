
<div>
  @foreach(var year in Model.Years) {
  <a href="#@year.Name">@year.Name</a> @:|
  }
</div>

@foreach(var year in Model.Years) {
  <a name="@year.Name" />
  <h2>@year.Name</h2>

  <ul>
  @foreach(var month in year.Months) {

  <!--<ul>-->
    foreach(var post in month.Posts) {
    <li>
      <a href="@post.UrlPath">@post.Title</a> (@month.Name)
    </li>
    }
 <!-- </ul>-->

  }
  </ul>
}